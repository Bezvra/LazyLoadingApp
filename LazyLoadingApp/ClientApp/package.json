{
  "name": "LazyLoadingApp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "ts:css": "tcm -s -c -p src/**/*.module.scss",
    "start": "concurrently \"npm:ts:css -- -w\" \"react-scripts start\"",
    "build": "npm run ts:css && react-scripts build",
    "test": "concurrently \"npm:ts:css -- -w\" \"cross-env CI=true react-scripts test --env=jsdom\"",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react-redux": "^7.1.0",
    "react-scripts": "5.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/history": "^4.7.2",
    "@types/jest": "24.0.16",
    "@types/node": "12.6.9",
    "@types/react": "16.8.24",
    "@types/react-dom": "16.8.5",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "concurrently": "^4.1.1",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "redux-devtools-extension": "^2.13.8",
    "serve": "^11.1.0",
    "typed-css-modules": "^0.6.0",
    "typed-scss-modules": "^0.0.11",
    "typescript": "^3.5.3"
  }
}
